python train.py \
--logtostderr \
--train_dir=/home/joshua/gits/IoTPark/training/train_dir/[trained model dir] \
--pipeline_config_path=/home/joshua/gits/IoTPark/training/feature_extractors/[feature extractor]/[feature extractor].config \
> train.log 2>&1 &

python export_inference_graph.py \
--input_type image_tensor \
--pipeline_config_path /home/joshua/gits/IoTPark/training/feature_extractors/[feature extractor]/[feature extractor].config \
--trained_checkpoint_prefix /home/joshua/gits/IoTPark/training/train_dir/[trained model dir]/model.ckpt-##### \
--output_directory /home/joshua/gits/IoTPark/training/frozen/

python eval.py --logtostderr \
--checkpoint_dir=/home/joshua/gits/IoTPark/training/train_dir/[trained model dir] \
--eval_dir=/home/joshua/gits/IoTPark/training/eval_dir/[eval model dir] \
--pipeline_config_path=/home/joshua/gits/IoTPark/training/feature_extractors/[feature extractor]/[feature extractor].config
